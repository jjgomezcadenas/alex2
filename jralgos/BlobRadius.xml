<?xml version="1.0" encoding="ISO-8859-1"?>
<BlobRadius>
  <Param>
    <name>fName</name>
    <dataType>string</dataType>
    <value>BlobRadius</value>
  </Param>
 
  <Param>
    <name>fMaxDist</name>
    <dataType>double</dataType>
    <value>20.</value>
  </Param>

  <Param>
    <name>fRadMin</name>
    <dataType>double</dataType>
    <value>20.0</value>
  </Param>

  <Param>
    <name>fRadMax</name>
    <dataType>double</dataType>
    <value>20.1</value>
  </Param>

  <Param>
    <name>fRadInc</name>
    <dataType>double</dataType>
    <value>1.0</value>
  </Param>

 <!-- <Param>
    <name>fPMax</name>
    <dataType>double</dataType>
    <value>2.9135</value>
  </Param>
  <Param>
    <name>fPMin</name>
    <dataType>double</dataType>
    <value>0.25</value>
  </Param> -->
  <H1D>
    <name>fH1_nth_pe</name>
    <title>Number of true hits per primary electron</title>
    <nbinsx>50</nbinsx>
    <xlow>0.0</xlow>
    <xup>500.0</xup>
  </H1D>


  <H1D>
    <name>fH1_ntrck</name>
    <title>Number of tracks </title>
    <nbinsx>20</nbinsx>
    <xlow>0.0</xlow>
    <xup>20.0</xup>
  </H1D>


  <H1D>
    <name>fH1_mindist</name>
    <title>Minimum distance between track and hit</title>
    <nbinsx>250</nbinsx>
    <xlow>0.0</xlow>
    <xup>50.0</xup>
  </H1D>

  <H1D>
    <name>fH1_mindist_in</name>
    <title>Minimum distance between track and hit for hits added</title>
    <nbinsx>50</nbinsx>
    <xlow>0.0</xlow>
    <xup>500.0</xup>
  </H1D>

  <H1D>
    <name>fH1_mindist_out</name>
    <title>Minimum distance between track and hit for hits not added</title>
    <nbinsx>50</nbinsx>
    <xlow>0.0</xlow>
    <xup>500.0</xup>
  </H1D>

  
  <H2D>
    <name>fH2_nth_2e</name>
    <title>True hits of two electrons</title>
    <nbinsx>100</nbinsx>
    <nbinsy>100</nbinsy>
    <xlow>0.0</xlow>
    <xup>500.0</xup>
    <ylow>00.</ylow>
    <yup>500.0</yup>
  </H2D>

  <H2D>
    <name>fH2_radEnergy1</name>
    <title>Blob radius vs. energy in blob 1</title>
    <nbinsx>100</nbinsx>
    <nbinsy>100</nbinsy>
    <xlow>0.0</xlow>
    <xup>100.0</xup>
    <ylow>0.0</ylow>
    <yup>2.8</yup>
  </H2D>

  <H2D>
    <name>fH2_radEnergy2</name>
    <title>Blob radius vs. energy in blob 2</title>
    <nbinsx>100</nbinsx>
    <nbinsy>100</nbinsy>
    <xlow>0.0</xlow>
    <xup>100.0</xup>
    <ylow>0.0</ylow>
    <yup>2.8</yup>
  </H2D>

  <H2D>
    <name>fH2_blobEnergy</name>
    <title>Blob 1 energy vs. blob 2 energy</title>
    <nbinsx>100</nbinsx>
    <nbinsy>100</nbinsy>
    <xlow>0.0</xlow>
    <xup>2.4</xup>
    <ylow>0.0</ylow>
    <yup>2.8</yup>
  </H2D>
  
</BlobRadius>
